{
  "name": "gannbaruzoi-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-fetch": "^2.2.1",
    "register-service-worker": "^1.2.0",
    "uuid": "^3.2.1",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-stage-1": "7.0.0-beta.46",
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.10",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "@vue/eslint-config-prettier": "^3.0.0-beta.10",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^7.1.0",
    "vue-template-compiler": "^2.5.13",
    "wallaby-vue-compiler": "^1.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.graphql": [
      "prettier --write",
      "git add"
    ]
  }
